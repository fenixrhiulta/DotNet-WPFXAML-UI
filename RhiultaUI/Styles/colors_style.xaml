<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="controlBackgroundColor">#EEE</Color>
    <Color x:Key="controlForegroundColor">#222</Color>

    <Color x:Key="primaryColor">#00796B</Color>
    <Color x:Key="primaryDarkColor">#004D40</Color>

    <Color x:Key="primaryInvertColor">#EEE</Color>

    <Color x:Key="highlightColor">#FDD835</Color>
    <Color x:Key="highlightInvertColor">#FFFDE7</Color>

    <Color x:Key="redColor">#D32F2F</Color>

    <SolidColorBrush x:Key="primaryColorSB" Color="{DynamicResource primaryColor}" />
    <SolidColorBrush x:Key="primaryColorSB_80" Opacity="0.8" Color="{DynamicResource primaryColor}" />
    <SolidColorBrush x:Key="primaryDarkColorSB" Color="{DynamicResource primaryDarkColor}" />
    <SolidColorBrush x:Key="primaryInvertColorSB" Color="{DynamicResource primaryInvertColor}" />
    <SolidColorBrush x:Key="controlBackgroundColorSB" Color="{DynamicResource controlBackgroundColor}" />
    <SolidColorBrush x:Key="controlForegroundColorSB" Color="{DynamicResource controlForegroundColor}" />
    <SolidColorBrush x:Key="highlightColorSB" Color="{DynamicResource highlightColor}" />

    <SolidColorBrush x:Key="redColorSB" Color="{DynamicResource redColor}" />


